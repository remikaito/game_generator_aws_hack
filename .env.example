# ===========================================
# Game Prototype Generator - Environment Variables
# ===========================================
# Copy this file to .env and fill in your API keys

# Anthropic Claude API
# Get your key at: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-your-key-here

# TRIPO 3D API (Text-to-3D generation)
# Get your key at: https://www.tripo3d.ai/
TRIPO_API_KEY=your-tripo-key-here

# Blockade Labs Skybox AI
# Get your key at: https://www.blockadelabs.com/
BLOCKADE_API_KEY=your-blockade-key-here

# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# Asset Storage Path
ASSETS_PATH=./assets
